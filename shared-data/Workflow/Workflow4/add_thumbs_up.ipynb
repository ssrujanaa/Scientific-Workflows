    "#!/usr/bin/env python3\n",
    "import emoji\n",
    "import sys\n",
    "\n",
    "input_file = sys.argv[1]\n",
    "output_file = sys.argv[2]\n",
    "\n",
    "with open(input_file, 'r') as f,open(output_file,'w',encoding='utf-8-sig') as f1:\n",
    "    input_txt = f.readlines()\n",
    "    f1.write(input_txt)\n",
    "    f1.write(emoji.emojize(':thumbs_up:'))"
   ]
