#!/usr/bin/env python3
import sys

input_filename  = sys.argv[1]
output_filename = sys.argv[2]
count = 0 

with open(input_filename, "r") as in_file:
    num = in_file.read().split('\n')
    for n in num:
        count = count+1
        
with open(output_filename, "w") as out_file:
    out_file.write(str(count-1))


